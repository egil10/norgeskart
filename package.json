{
  "name": "norgeskart",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:with-data": "npm run generate:data && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate:data": "tsx src/lib/data/fetchPeople.ts",
    "generate:data:list": "tsx src/lib/data/fetchPeople.ts --list",
    "generate:data:merge": "tsx src/lib/data/fetchPeople.ts --merge",
    "generate:wikipedia": "tsx src/lib/data/fetchWikipediaCategories.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/d3": "^7.4.3",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.3"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "d3": "^7.8.5",
    "fuse.js": "^7.0.0",
    "lucide-svelte": "^0.447.0"
  },
  "type": "module"
}